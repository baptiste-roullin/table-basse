{
	"compilerOptions": {
		"removeComments": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"target": "es2019",
		"module": "commonjs",
		"outDir": "./build",
		"noImplicitAny": false,
		"esModuleInterop": true,
		"strict": true,
		"sourceMap": true,
		"strictNullChecks": false,
		"noImplicitReturns": true,
		"lib": [
			"ES2018",
			"es2019",
			"DOM",
			"ES6",
			"DOM.Iterable",
			"ScriptHost"
		]
	},
	"exclude": [
		"node_modules",
		"front"
	]
}