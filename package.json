{
	"name": "table-basse",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon --ignore front/ --exec 'npx ts-node' ./src/main.ts",
		"start": "node ./build/main.js",
		"heroku-postbuild": "npx tsc && cd front && npm run build"
	},
	"workspaces": [
		"*"
	],
	"engines": {
		"node": "^15.0.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"cloudinary": "^1.23.0",
		"connect-history-api-fallback": "^1.6.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"express-promise-router": "^4.0.1",
		"fsevents": "^2.x.x",
		"got": "^11.8.1",
		"p-map": "^4.0.0",
		"pg": "^8.5.1",
		"pg-hstore": "^2.3.3",
		"puppeteer": "^5.3.1",
		"sanitize-filename": "^1.6.3",
		"sequelize": "^6.3.5",
		"serve-static": "^1.14.1"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.23",
		"@types/cors": "^2.8.9",
		"@types/express": "^4.17.8",
		"@types/node": "^14.14.14",
		"@types/puppeteer": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"cross-env": "^7.0.3",
		"eslint": "^6.7.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"optionalDependencies": {
		"fsevents": "^2.x.x"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"eslint:recommended"
		],
		"parserOptions": {
			"ecmaVersion": 2020
		},
		"rules": {
			"no-unused-vars": "off",
			"no-redeclare": "off",
			"no-debugger": 1,
			"no-unexpected-multiline": 1,
			"semi": 1,
			"@typescript-eslint/semi": 1
		}
	},
	"gitHooks": {
		"post-commit": ""
	}
}